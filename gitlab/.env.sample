# Debug and Logging
DEBUG=false  # Whether to enable debug mode

# Database Configuration
DB_ADAPTER=postgresql  # Database adapter type
DB_HOST=postgresql  # Database hostname
DB_PORT=5432  # Database port number
DB_USER=gitlab  # Database username
DB_PASS=password  # Database password
DB_NAME=gitlabhq_production  # Database name

# Redis Configuration
REDIS_HOST=redis  # Redis hostname
REDIS_PORT=6379  # Redis port number

# Timezone Configuration
TZ=Asia/Kolkata  # System timezone
GITLAB_TIMEZONE=Kolkata  # GitLab application timezone

# HTTPS and SSL Configuration
GITLAB_HTTPS=false  # Whether to enable HTTPS
SSL_SELF_SIGNED=false  # Whether to use a self-signed certificate

# GitLab General Configuration
GITLAB_HOST=localhost  # GitLab hostname
GITLAB_PORT=10080  # GitLab HTTP port
GITLAB_SSH_PORT=10022  # GitLab SSH port
GITLAB_RELATIVE_URL_ROOT=  # GitLab relative URL root
GITLAB_SECRETS_DB_KEY_BASE=long-and-random-alphanumeric-string  # Database key base
GITLAB_SECRETS_SECRET_KEY_BASE=long-and-random-alphanumeric-string  # Application secret key base
GITLAB_SECRETS_OTP_KEY_BASE=long-and-random-alphanumeric-string  # OTP key base

# GitLab Admin User Configuration
GITLAB_ROOT_PASSWORD=  # GitLab root user password
GITLAB_ROOT_EMAIL=  # GitLab root user email

# GitLab Notification Configuration
GITLAB_NOTIFY_ON_BROKEN_BUILDS=true  # Whether to notify on broken builds
GITLAB_NOTIFY_PUSHER=false  # Whether to notify the pusher

# GitLab Email Configuration
GITLAB_EMAIL=notifications@example.com  # GitLab notification email address
GITLAB_EMAIL_REPLY_TO=noreply@example.com  # GitLab notification email reply-to address
GITLAB_INCOMING_EMAIL_ADDRESS=reply@example.com  # GitLab incoming email address

# GitLab Backup Configuration
GITLAB_BACKUP_SCHEDULE=daily  # Backup schedule
GITLAB_BACKUP_TIME=01:00  # Backup time

# SMTP Configuration
SMTP_ENABLED=false  # Whether to enable SMTP
SMTP_DOMAIN=www.example.com  # SMTP domain
SMTP_HOST=smtp.gmail.com  # SMTP host
SMTP_PORT=587  # SMTP port
SMTP_USER=mailer@example.com  # SMTP username
SMTP_PASS=password  # SMTP password
SMTP_STARTTLS=true  # Whether to enable STARTTLS
SMTP_AUTHENTICATION=login  # SMTP authentication method

# IMAP Configuration
IMAP_ENABLED=false  # Whether to enable IMAP
IMAP_HOST=imap.gmail.com  # IMAP host
IMAP_PORT=993  # IMAP port
IMAP_USER=mailer@example.com  # IMAP username
IMAP_PASS=password  # IMAP password
IMAP_SSL=true  # Whether to enable SSL
IMAP_STARTTLS=false  # Whether to enable STARTTLS

# OAuth Configuration
OAUTH_ENABLED=false  # Whether to enable OAuth
OAUTH_AUTO_SIGN_IN_WITH_PROVIDER=  # Auto sign-in with provider
OAUTH_ALLOW_SSO=  # Whether to allow single sign-on
OAUTH_BLOCK_AUTO_CREATED_USERS=true  # Whether to block auto-created users
OAUTH_AUTO_LINK_LDAP_USER=false  # Whether to auto-link LDAP users
OAUTH_AUTO_LINK_SAML_USER=false  # Whether to auto-link SAML users
OAUTH_EXTERNAL_PROVIDERS=  # External providers

# OAuth CAS3 Configuration
OAUTH_CAS3_LABEL=cas3  # CAS3 label
OAUTH_CAS3_SERVER=  # CAS3 server
OAUTH_CAS3_DISABLE_SSL_VERIFICATION=false  # Whether to disable SSL verification for CAS3
OAUTH_CAS3_LOGIN_URL=/cas/login  # CAS3 login URL
OAUTH_CAS3_VALIDATE_URL=/cas/p3/serviceValidate  # CAS3 validate URL
OAUTH_CAS3_LOGOUT_URL=/cas/logout  # CAS3 logout URL

# OAuth Google Configuration
OAUTH_GOOGLE_API_KEY=  # Google API key
OAUTH_GOOGLE_APP_SECRET=  # Google app secret
OAUTH_GOOGLE_RESTRICT_DOMAIN=  # Google restrict domain

# OAuth Facebook Configuration
OAUTH_FACEBOOK_API_KEY=  # Facebook API key
OAUTH_FACEBOOK_APP_SECRET=  # Facebook app secret

# OAuth Twitter Configuration
OAUTH_TWITTER_API_KEY=  # Twitter API key
OAUTH_TWITTER_APP_SECRET=  # Twitter app secret

# OAuth GitHub Configuration
OAUTH_GITHUB_API_KEY=  # GitHub API key
OAUTH_GITHUB_APP_SECRET=  # GitHub app secret
OAUTH_GITHUB_URL=  # GitHub URL
OAUTH_GITHUB_VERIFY_SSL=  # GitHub SSL verification

# OAuth GitLab Configuration
OAUTH_GITLAB_API_KEY=  # GitLab API key
OAUTH_GITLAB_APP_SECRET=  # GitLab app secret

# OAuth Bitbucket Configuration
OAUTH_BITBUCKET_API_KEY=  # Bitbucket API key
OAUTH_BITBUCKET_APP_SECRET=  # Bitbucket app secret
OAUTH_BITBUCKET_URL=  # Bitbucket URL

# OAuth SAML Configuration
OAUTH_SAML_ASSERTION_CONSUMER_SERVICE_URL=  # SAML assertion consumer service URL
OAUTH_SAML_IDP_CERT_FINGERPRINT=  # SAML IDP certificate fingerprint
OAUTH_SAML_IDP_SSO_TARGET_URL=  # SAML IDP SSO target URL
OAUTH_SAML_ISSUER=  # SAML issuer
OAUTH_SAML_LABEL="Our SAML Provider"  # SAML label
OAUTH_SAML_NAME_IDENTIFIER_FORMAT=urn:oasis:names:tc:SAML:2.0:nameid-format:transient  # SAML name identifier format
OAUTH_SAML_GROUPS_ATTRIBUTE=  # SAML groups attribute
OAUTH_SAML_EXTERNAL_GROUPS=  # SAML external groups
OAUTH_SAML_ATTRIBUTE_STATEMENTS_EMAIL=  # SAML attribute statements - email
OAUTH_SAML_ATTRIBUTE_STATEMENTS_NAME=  # SAML attribute statements - name
OAUTH_SAML_ATTRIBUTE_STATEMENTS_USERNAME=  # SAML attribute statements - username
OAUTH_SAML_ATTRIBUTE_STATEMENTS_FIRST_NAME=  # SAML attribute statements - first name
OAUTH_SAML_ATTRIBUTE_STATEMENTS_LAST_NAME=  # SAML attribute statements - last name

# OAuth Crowd Configuration
OAUTH_CROWD_SERVER_URL=  # Crowd server URL
OAUTH_CROWD_APP_NAME=  # Crowd application name
OAUTH_CROWD_APP_PASSWORD=  # Crowd application password

# OAuth Auth0 Configuration
OAUTH_AUTH0_CLIENT_ID=  # Auth0 client ID
OAUTH_AUTH0_CLIENT_SECRET=  # Auth0 client secret
OAUTH_AUTH0_DOMAIN=  # Auth0 domain
OAUTH_AUTH0_SCOPE=  # Auth0 scope

# OAuth Azure Configuration
OAUTH_AZURE_API_KEY=  # Azure API key
OAUTH_AZURE_API_SECRET=  # Azure API secret
OAUTH_AZURE_TENANT_ID=  # Azure tenant ID